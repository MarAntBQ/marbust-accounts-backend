        <nav class="header__nav js-header-nav">
            <ul>
                <% if (moduleName === 'dashboard') { %>
                    <li><a href="/dashboard/" class="<%= pagetoLoad === 'dashboard/home' ? 'active' : '' %>">Dashboard</a></li>
