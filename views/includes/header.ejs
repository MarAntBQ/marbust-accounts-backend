                    <li><a href="/account-setup/logout">Logout</a></li>
                <% } else if (moduleName === 'admin') { %>
                    <li><a href="/dashboard/">Dashboard</a></li>
                    <li><a href="/admin/" class="<%= pagetoLoad === 'admin/home' ? 'active' : '' %>">Admin</a></li>
                    <li><a href="/admin/users" class="<%= pagetoLoad === 'admin/users' || pagetoLoad === 'admin/user-details'  ? 'active' : '' %>">Users</a></li>
